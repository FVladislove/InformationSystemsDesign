@model IEnumerable<InformationSystemsDesign.Models.GLPR>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NmPr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CdTp)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NmPr)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CdTp)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CdPr">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CdPr">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CdPr">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
